<?xml version="1.0" encoding="utf-8"?>
<com.clibrary.recycledev.RenovateLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:id="@id/base_page"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <study.rq.com.customcalendar.oldcalendar.CalendarFrame
            android:id="@id/base_tip"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <study.rq.com.customcalendar.oldcalendar.CalendarRecycler
            android:id="@id/base_list"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:overScrollMode="never" />
    </LinearLayout>
</com.clibrary.recycledev.RenovateLayout>